{
  "compilerOptions": {
    "outDir": "dist/",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "declaration": true,
    "sourceMap": true
  },
  "include": ["polyfill/index.ts"],
  "exclude": [
    "polyfill/*.spec.ts",
    "polyfill/constants.ts",
    "polyfill/performance.ts"
  ]
}
